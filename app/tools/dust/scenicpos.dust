//数据
let _data = {json|s};

class ScenicPos {~lb}
    constructor (cfg) {~lb}
        this.cfg = cfg;
    {~rb}
    get id () {return this.cfg.id}
    get x () {return this.cfg.x}
    get y () {return this.cfg.y}

    static Get(id) { return id in _data ? new ScenicPos(_data[id]) : null;}
{~rb}

module.exports = ScenicPos;