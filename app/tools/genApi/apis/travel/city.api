import ../base.api

class CityPer {
    prop cityname: string   //城市名
    prop cityper: number   //城市完成度
}

class ProvencePer {
    prop proLetter: string //该省份的首字母（大写）
    prop proName: string   //省名称
    prop citys: CityPer[]  //该省下去过的城市及完成度
}

//拉取城市完成度列表
api CityListPer {
    output data:Provence[]
}

