import ../base.api

class CityPer {
    prop cityname: string   //城市名
    prop cityper: number   //城市完成度
}

class ProvencePer {
    prop proLetter: string //该省份的首字母（大写）
    prop proName: string   //省名称
    prop citys: CityPer[]  //该省下去过的城市及完成度
}

//随机机票中的城市(需要除去用户已经去过的城市)
class RandomCity {
    prop city:string
}

//拉取城市完成度列表
api CityListPer {
    output data:Provence[]
}

//拉取随机机票的城市列表(需要除去用户已经去过的城市)
api RandomCityList {
    output city:RandomCity[]
}