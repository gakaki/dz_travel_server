{
  "extends": "eslint-config-egg",
  "rules": {
    "indent": 0,
    "strict": 0,
    "linebreak-style": 0,
    "no-unused-vars": 0,
    "prefer-const": 0,
    "spaced-comment": 0,
    "semi":0,
    "keyword-spacing": 0,
    "quotes": 0,
    "eqeqeq": 0,
    "no-trailing-spaces": 0,
    "no-multiple-empty-lines": 0,
    "eol-last": 0
  }
}
